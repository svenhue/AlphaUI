{
    "compilerOptions": {
      "baseUrl": "src",
      "paths": {
        "@assets/*": ["assets/*"],
        "@plugins/*": ["plugins/*"],
        "@utils/*": ["utils/*"],
        "@/src*": ["/*"]
      },
      "esModuleInterop": true,
      "module": "ESNext",
      "moduleResolution": "Node",
      "target": "ES2020",
      "lib": ["DOM", "DOM.Iterable", "ESNext"],
      "noEmit": true,
      "allowImportingTsExtensions":true,
      "resolveJsonModule": true,
    },
  
    }